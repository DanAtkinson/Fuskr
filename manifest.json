{
	"manifest_version": 2,
	"name": "Fuskr",
	"version": "2.5",
	"description": "Create photo galleries from a single image based on the image name.",
	"omnibox": { "keyword" : "fuskr" },
	"background": {
		"scripts": [
			"Scripts/fuskr.js",
			"Scripts/background.js"
		]
	},
	/*"page_action": {
		"default_icon": { 
			"19": "Images/16x16.png",
			"38": "Images/48x48.png"
		},
		"default_title": "Fuskr"//,
		//"default_popup": "popup.html"
	},*/
	"options_page": "Html/options.htm",
	"incognito": "split",
	"omnibox": { "keyword" : "fuskr" },
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"contextMenus",
		"\u003Call_urls\u003E"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
	"homepage_url": "http://danatkinson.github.io/Fuskr/",
	"offline_enabled": false,
	"icons": {
		"16": "Images/16x16.png",
		"48": "Images/48x48.png",
		"128": "Images/128x128.png"
	}
}
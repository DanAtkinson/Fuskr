{
	"manifest_version": 2,
	"name": "Fuskr",
	"version": "1.9.2",
	"description": "A Chromeified port of FireFusk",
	"background": {
		"scripts": [
			"Scripts/fuskr.js",
			"Scripts/background.js"
		]
	},
	"options_page": "Html/options.htm",
	"incognito": "split",
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"contextMenus"
	],
	"homepage_url": "http://fuskr.com/",
	"offline_enabled": false,
	"icons" : {
		"16" : "Images/Icon-16.png",
		"48" : "Images/Icon-48.png",
		"128" : "Images/Icon-128.png"
	}
}